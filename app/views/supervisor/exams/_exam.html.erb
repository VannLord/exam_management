<tr>
  <td><%= exam.name %></td>
  <td><%= exam.subject_name %></td>
  <td><%= l(exam.created_at, format: :long) %></td>
  <td><%= exam.time %></td>
  <td><%= exam.total_number_questions %></td>
  <td>
      <%= link_to t("exams.info"), exam_path(exam), class: "btn btn-primary", role: "button" %>
  </td>
  <!-- <td>
      <%= link_to t("exams.edit"), "#", class: "btn btn-success", role: "button" %>
  </td> -->
  <td>
      <%= link_to t("exams.delete"), exam_path(exam), method: :delete, class: "btn btn-danger", role: "button", data: { confirm: "You sure?" } %>
  </td>
</tr>
