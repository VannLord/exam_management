<tr>
  <td><%= l(user_exam.created_at, format: :long) %></td>
  <td><%= user_exam.subject.name %></td>
  <td>
    <span class="<%= class_for_user_exam_status user_exam %>">
      <%= user_exam.status %>
    </span>
  </td>
  <td><%= user_exam.exam.time %></td>
  <td><%= user_exam.exam.exam_questions.size %></td>
  <td><%= convert_num_to_time user_exam.spent_time %></td>
  <td><%= user_exam.total_score || "-" %></td>
  <td>
    <%= link_to  t("user_exams.view"), trainee_user_exam_path(@trainee, user_exam), class: "btn btn-success button" %>
  </td>
</tr>
